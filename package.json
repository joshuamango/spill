    "build": "react-scripts build"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "eslint": "^5.12.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "lodash": ">=4.17.19",
    "nodemon": "^2.0.4",
    "react": "^16.12.0",
    "react-spring": "^8.0.27",
    "reactdom": "^2.0.0",
    "sqlite3": "^4.2.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.2"
  }
}
